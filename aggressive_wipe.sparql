PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX loan: <http://www.semanticweb.org/ontology/loan_approval#>

DELETE { ?s ?p ?o }
WHERE {
  ?s ?p ?o .
  FILTER(
    EXISTS { ?s rdf:type/rdfs:subClassOf* loan:Applicant } ||
    EXISTS { ?s rdf:type/rdfs:subClassOf* loan:Loan } ||
    STRSTARTS(STR(?s), "http://www.semanticweb.org/ontology/loan_approval#App_") ||
    STRSTARTS(STR(?s), "http://www.semanticweb.org/ontology/loan_approval#Loan_") ||
    STRSTARTS(STR(?s), "http://www.semanticweb.org/ontology/loan_approval#PLoan") ||
    STRSTARTS(STR(?s), "http://www.semanticweb.org/ontology/loan_approval#HLoan") ||
    STRSTARTS(STR(?s), "http://www.semanticweb.org/ontology/loan_approval#ELoan")
  )
  # Keep base classes
  FILTER(?s != loan:Applicant && ?s != loan:Loan && ?s != loan:SalariedEmployee && ?s != loan:SelfEmployed && ?s != loan:Retiree && ?s != loan:Student && ?s != loan:PersonalLoan && ?s != loan:HousingLoan && ?s != loan:EducationLoan && ?s != loan:GoldLoan && ?s != loan:VanithaAruna && ?s != loan:InterestFreeStudentLoan)
}
